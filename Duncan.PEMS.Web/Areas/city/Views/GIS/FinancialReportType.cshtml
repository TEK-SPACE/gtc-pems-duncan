    <script type="text/javascript">
        function getCashAggregate(value) {
            
            var datasource = $("#grdCustomerDetails").data("kendoGrid").dataSource;
            var cashResult;
            var creditResult;
            var smartResult;
            var phoneResult;
            
            $(datasource.view()).each(function (index, element) {
                //compares view value with the current template value
                if (element.value == value) {
                    cashResult = element.aggregates.cashAmount.sum; //gets the aggregate value
                }

            });

           
            return "Cash Total: $" + cashResult / 100;
        }

        function getCreditAggregate(value) {

