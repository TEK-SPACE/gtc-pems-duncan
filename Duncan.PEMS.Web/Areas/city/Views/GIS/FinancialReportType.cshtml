                //compares view value with the current template value
                if (element.value == value) {
                    creditResult = element.aggregates.creditCardAmount.sum; //gets the aggregate value
                }
            });

            return "Credit Card Total: $" + creditResult / 100;
        }

        function getSmartAggregate(value) {

